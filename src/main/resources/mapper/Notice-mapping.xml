<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="Notice">
    <select id="Notice.getList" parameterType="NoticeVo"    resultType="NoticeVo">
        select *
        from notice
        where 1=1
    </select>
</mapper>